#!/bin/execlineb -P
s6-setuidgid %I
backtick -n HOME { homeof %I }
importas HOME HOME
if { rsync -a --no-t ${HOME}/.config/service /run/user/%I }
cd /run/user/%I/service
emptyenv -p
backtick -D /bin/sh -n SHELL {
  pipeline { getent passwd %I }
  cut -d: -f7
}
export HOME ${HOME}
export LOGNAME %I
export USER %I
export XDG_RUNTIME_DIR /run/user/%I
fdmove -c 2 1
s6-svscan -St0
