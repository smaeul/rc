if {
  cd /etc/net/routes
  s6-envdir -f .
  elglob -0s list *
  forx iface { ${list} }
  importas -u iface iface
  importas -su -d"\n" ${iface} ${iface}
  forx route { ${${iface}} }
  importas -su route route
  ip route delete dev ${iface} ${route}
}
if {
  cd /etc/net/addresses
  s6-envdir -f .
  elglob -0s list *
  forx iface { ${list} }
  importas -u iface iface
  importas -su -d"\n" ${iface} ${iface}
  forx addr { ${${iface}} }
  importas -su addr addr
  ip address delete dev ${iface} ${addr}
}
if {
  cd /etc/net/interfaces
  s6-envdir -f .
  elglob -0s list *
  forx iface { ${list} }
  importas -u iface iface
  if -t { busybox grep -Fq ${iface} /proc/net/dev }
  ip link set ${iface} down
}
