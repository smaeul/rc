#!/bin/execlineb -P
if { busybox sleep 2 }
if {
  elglob irqs /proc/irq/*/enp1s0f*-0/../smp_affinity
  forx -p irq { ${irqs} }
  importas -u irq irq
  redirfd -w 1 ${irq}
  busybox printf 1
}
if {
  elglob irqs /proc/irq/*/enp1s0f*-1/../smp_affinity
  forx -p irq { ${irqs} }
  importas -u irq irq
  redirfd -w 1 ${irq}
  busybox printf 2
}
elglob irq /proc/irq/*/enp2s0/../smp_affinity
importas -u irq irq
redirfd -w 1 ${irq}
busybox printf 2
