background {
  if -t { busybox mountpoint -q /sys/fs/fuse/connections }
  busybox umount /sys/fs/fuse/connections
}
background {
  if -t { busybox mountpoint -q /sys/kernel/config }
  busybox umount /sys/kernel/config
}
background {
  if -t { busybox mountpoint -q /sys/kernel/debug }
  busybox umount /sys/kernel/debug
}
background {
  if -t { busybox mountpoint -q /sys/kernel/security }
  busybox umount /sys/kernel/security
}
wait { }
